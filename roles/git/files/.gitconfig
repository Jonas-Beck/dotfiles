[user]
	email = jonasb@outlook.dk
	name = Jonas Beck
	signingkey = ~/.ssh/id_ed25519.pub
[core]
	autocrlf = input
	excludesfile = ~/.gitignore
[includeIf "gitdir:~/Developer/VELUX/"]
    path = ~/Developer/VELUX/.gitconfig
[credential "https://github.com"]
	helper = 
	helper = !/opt/homebrew/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper = 
	helper = !/opt/homebrew/bin/gh auth git-credential
[commit]
	gpgSign = true
[gpg]
	format = ssh
[pull]
	rebase = true
